    <id>FBS.XF.Toolkit</id>
    <version>1.28.0</version>
    <title>FBS Xamarin Forms Toolkit</title>
    <description>The Fairwind Business Solutions XF toolkit is a .NET library that extends your .NET Xamarin applications with various controls, behaviours, collections, converters, extensions and other helper functions.</description>
    <releaseNotes></releaseNotes>
    <authors>Colin Blakey</authors>
    <owners>Colin.Blakey</owners>
    <license type="expression">MIT</license>
    <licenseUrl>https://licenses.nuget.org/MIT</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <copyright>Copyright Â© Fairwinds Business Solutions</copyright>
    <!-- <icon>Fairwind.png</icon> -->
    <readme>README.md</readme>
    <tags>Xamarin Forms Controls</tags>
    <dependencies>
        <group targetFramework=".NETStandard2.1">
            <dependency id="FluentValidation" version="11.8.0" exclude="Build,Analyzers" />
            <dependency id="Microsoft.EntityFrameworkCore.Sqlite" version="5.0.17" exclude="Build,Analyzers" />
            <dependency id="PropertyChanged.Fody" version="4.1.0" exclude="Build,Analyzers" />
            <dependency id="Rg.Plugins.Popup" version="2.1.0" exclude="Build,Analyzers" />
            <dependency id="SQLitePCLRaw.bundle_e_sqlite3" version="2.1.6" exclude="Build,Analyzers" />
            <dependency id="SkiaSharp" version="2.80.3" exclude="Build,Analyzers" />
